{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:List*"
            ],
            "Resource": [
                "arn:aws:s3:::mint-123456789-eu-west-1",
                "arn:aws:s3:::mint-123456789-eu-west-1/*"
            ]
        }
    ]
}
